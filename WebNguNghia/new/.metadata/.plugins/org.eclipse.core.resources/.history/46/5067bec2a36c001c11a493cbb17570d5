	static Model creaModel8() {
		// some definitions
		String personURI = "http://www.w3.org/Khoa/CNTT";
		String KhoaK41IRI = "http://ww.w3.org/Khoa/K41";
		String KhoaK42IRI = "http://ww.w3.org/Khoa/K42";
		// create an empty Model
		Model model8 = ModelFactory.createDefaultModel();

		// create the resource
		// and add the properties cascading style
		Resource lnt = model8.createResource(personURI).addProperty(VCARD.N,
				model8.createResource(KhoaK41IRI));

		return model8;
		//model.write(System.out);
	}
	
	
///////////////////////////////////////////	
	
	static void createAndPrintModel() {
		Model model =creaModel();
		Model model2 =creaModel2();
		Model model3 =creaModel3();
		Model model4 =creaModel4();
		Model model5 =creaModel5();
		Model model6 =creaModel6();
		Model model7 =creaModel7();
		Model model8 =creaModel8();
		// In nội dung của model dưới dạng mặc định (XML-RDF)
		model.write(System.out);
		model2.write(System.out);
		model3.write(System.out);
		model4.write(System.out);
		model5.write(System.out);
		model6.write(System.out);
		model7.write(System.out);
		model8.write(System.out);
	}
	
	static void createAndPrintModel2() {
		System.out.println("Xuất dữ liệu theo dạng bộ 3");
		Model model1= creaModel();
		StringWriter sw= new StringWriter();
		RDFDataMgr.write(sw, model1, org.apache.jena.riot.Lang.NTRIPLES);
		System.out.println("Nội dung dữ liệu 1");
		System.out.println(sw);
		
		Model model2= creaModel2();
		StringWriter sw2= new StringWriter();
		RDFDataMgr.write(sw2, model2, org.apache.jena.riot.Lang.NTRIPLES);
		System.out.println("Nội dung dữ liệu 2");
		System.out.println(sw2);
		
		Model model3= creaModel3();
		StringWriter sw3= new StringWriter();
		RDFDataMgr.write(sw3, model3, org.apache.jena.riot.Lang.NTRIPLES);
		System.out.println("Nội dung dữ liệu 3");
		System.out.println(sw3);
		
		Model model4= creaModel4();
		StringWriter sw4= new StringWriter();
		RDFDataMgr.write(sw4, model4, org.apache.jena.riot.Lang.NTRIPLES);
		System.out.println("Nội dung dữ liệu 4");
		System.out.println(sw4);
		
		Model model5= creaModel5();
		StringWriter sw5= new StringWriter();
		RDFDataMgr.write(sw5, model5, org.apache.jena.riot.Lang.NTRIPLES);
		System.out.println("Nội dung dữ liệu 5");
		System.out.println(sw5);
		
		Model model6= creaModel6();
		StringWriter sw6= new StringWriter();
		RDFDataMgr.write(sw6, model6, org.apache.jena.riot.Lang.NTRIPLES);
		System.out.println("Nội dung dữ liệu 6");
		System.out.println(sw6);
		
		Model model7= creaModel7();
		StringWriter sw7= new StringWriter();
		RDFDataMgr.write(sw7, model7, org.apache.jena.riot.Lang.NTRIPLES);
		System.out.println("Nội dung dữ liệu 7");
		System.out.println(sw7);
		
		Model model8= creaModel8();
		StringWriter sw8= new StringWriter();
		RDFDataMgr.write(sw8, model8, org.apache.jena.riot.Lang.NTRIPLES);
		System.out.println("Nội dung dữ liệu 8");
		System.out.println(sw8);
	}
	
	
	public static void main(String[] args) {
		createAndPrintModel();
		createAndPrintModel2();
	}
}
