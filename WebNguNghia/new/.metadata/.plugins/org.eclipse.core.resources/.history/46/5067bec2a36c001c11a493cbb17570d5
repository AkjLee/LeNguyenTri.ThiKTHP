package com.LNTri.jena;

import java.io.StringWriter;

import org.apache.jena.rdf.model.Model;
import org.apache.jena.rdf.model.ModelFactory;
import org.apache.jena.rdf.model.Resource;
import org.apache.jena.riot.RDFDataMgr;
import org.apache.jena.vocabulary.DC;
import org.apache.jena.vocabulary.VCARD;

public class baithi {

	static Model creaModel() {
		// some definitions
		String personURI = "http://www.w3.org/Giangvien/LeMyCanh";
		String KhoaIRI = "http://ww.w3.org/Khoa/CNTT";
		String givenName = "Le";
		String familyName = "My Canh";
		String emailName = "LMCanh123@w3.org";
		String fullName = givenName + " " + familyName;

		// create an empty Model
		Model model1 = ModelFactory.createDefaultModel();

		// create the resource
		// and add the properties cascading style
		Resource lnt = model1.createResource(personURI).addProperty(VCARD.N,
				model1.createResource(KhoaIRI).addProperty(VCARD.EMAIL, emailName).addProperty(VCARD.NAME, fullName));

		return model1;
		//model.write(System.out);
	}
///////////////////////////////////////////
	static Model creaModel2() {
		// some definitions
		String personURI = "http://www.w3.org/Khoa/K42";
		String KhoaK42IRI = "http://ww.w3.org/LeNguyenTri";
		String givenName = "Le Nguyen";
		String familyName = "Tri";
		String mark  =" 8.00";
		String emailName = "lnTri1@w3.org";
		String fullName = givenName + " " + familyName;

		// create an empty Model
		Model model2 = ModelFactory.createDefaultModel();

		// create the resource
		// and add the properties cascading style
		Resource lnt = model2.createResource(personURI).addProperty(VCARD.N,
				model2.createResource(KhoaK42IRI).addProperty(VCARD.ADR, mark ).addProperty(VCARD.EMAIL, emailName).addProperty(VCARD.NAME, fullName));

		return model2;
		//model.write(System.out);
	}
	
	static Model creaModel3() {
			// some definitions
